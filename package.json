{
    "name": "webpack-playground",
    "version": "0.0.1",
    "description": "A playground",
    "main": "index.js",
    "module": "src/index.js",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "start": "webpack-dev-server --mode development --config ./carma-playground/webpack.config.js",
        "lint": "eslint ./src",
        "lint:fix": "eslint --fix ./src",
        "test": "jest",
        "test:ci": "jest --ci --json --silent --outputFile=test-report.json --testResultsProcessor=\"jest-junit\"  --forceExit",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage --colors",
        "build": "webpack  --mode development",
        "build:release": "cross-env-shell npm_registry_token=$npm_registry_token GENERATE_SOURCEMAP=false \"npm run gen:white-lbl && webpack --mode production\"",
        "build:watch": "webpack --mode development --watch"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "@babel/standalone": "^7.11.6",
        "ace-builds": "^1.4.12",
        "babel-loader": "^8.1.0",
        "core-js": "^3.6.5",
        "lodash": "^4.17.20",
        "react": "^16.13.1",
        "react-ace": "^9.1.4",
        "react-dom": "^16.13.1",
        "regenerator-runtime": "^0.13.7"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@testing-library/dom": "^7.24.3",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.0.4",
        "@testing-library/react-hooks": "^3.4.2",
        "@testing-library/user-event": "^12.1.7",
        "@types/jest": "^26.0.14",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.3.0",
        "babel-plugin-lodash": "^3.3.4",
        "css-loader": "^4.3.0",
        "eslint": "^7.10.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-jest": "^24.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "html-loader": "^1.3.1",
        "html-webpack-plugin": "^4.5.0",
        "jest": "^26.5.0",
        "jest-css-modules": "^2.1.0",
        "jest-in-case": "^1.0.2",
        "jest-junit": "^12.0.0",
        "node-sass": "^4.14.1",
        "prettier": "^2.1.2",
        "prettier-eslint": "^11.0.0",
        "sass-loader": "^10.0.2",
        "style-loader": "^1.3.0",
        "webpack": "^4.44.2",
        "webpack-bundle-analyzer": "^3.9.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-manifest-plugin": "^2.2.0"
    }
}

{
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "extends": ["prettier/standard", "prettier/react", "plugin:prettier/recommended"],
    "plugins": ["prettier", "jest"],
    "env": {
        "jest": true,
        "jest/globals": true
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
